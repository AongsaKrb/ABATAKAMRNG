-- StarterGui.AmaxGUI.Frame.Menu.MenuStatus
local function C_5()
local script = G2L["5"];
	local catalog = script.Parent.Parent.Frame
	
	local function resetmenu()
		catalog.General.ScrollingFrame.Visible = false
	end
	
	script.Parent.Changed:Connect(function (Value)
		print("Receive")
		if Value == "General" then
			resetmenu()
			catalog.General.ScrollingFrame.Visible = true
		end
	end)
end;
task.spawn(C_5);
-- StarterGui.AmaxGUI.Frame.ScrollingFrame.General.SetGeneral
local function C_9()
local script = G2L["9"];
	local Menu = script.Parent.Parent.Parent.Menu
	
	script.Parent.MouseButton1Click:Connect(function ()
		Menu.Value = "General"
		print(Menu)
	end)
end;
task.spawn(C_9);
-- StarterGui.AmaxGUI.Frame.Frame.General.ScrollingFrame.AutoKeep.Button.ToggleCheck
local function C_15()
local script = G2L["15"];
	local General = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.ToggleDetectFeature.General
	local ESP = General.AutoKeep
	
	script.Parent.Activated:Connect(function ()
		if ESP.Value == false then
			ESP.Value = true
			script.Parent.BackgroundColor3 = Color3.fromRGB(0, 91, 6)
		elseif ESP.Value == true then
			ESP.Value = false
			script.Parent.BackgroundColor3 = Color3.fromRGB(121, 0, 0)
		end
	end)
end;
task.spawn(C_15);
-- StarterGui.AmaxGUI.ToggleDetectFeature.General.AutoKeep.AutoKeep
local function C_1b()
local script = G2L["1b"];
	script.Parent.Changed:Connect(function()
		local targetValue = script.Parent.Value
	
		if targetValue == true then
			local running = true
	
			task.spawn(function()
				while running do
					task.spawn(function()
						local Players = game:GetService("Players")
						local ply = Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart")
	
						for _, v in pairs(workspace.NaturalIemSpawn.Objects:GetChildren()) do
							if v:FindFirstChild("ProximityPrompt") then
								v.CFrame = CFrame.new(ply.Position + Vector3.new(0, 3, 0))
								task.wait(1)
	
								if v.CanCollide then
									for _, pp in pairs(workspace.NaturalIemSpawn.Objects:GetDescendants()) do
										local prompt = pp:FindFirstChild("ProximityPrompt")
										if prompt and prompt.Enabled then
											for i = 1, 10 do
												pcall(function()
													fireproximityprompt(prompt)
												end)
												task.wait(0.1)
											end
	
											if prompt:IsDescendantOf(workspace) and prompt.Enabled then
												pp.CFrame = pp.CFrame + Vector3.new(0, 10, 0)
											end
										end
									end
									task.wait(5)
								end
	
								break
							end
						end
					end)
	
					task.wait(0.5) -- เว้นช่วงก่อน spawn รอบถัดไป
				end
			end)
	
			script.Parent.Changed:Wait()
			if script.Parent.Value == false then
				running = false
			end
		end
	end)
	
end;
task.spawn(C_1b);
-- StarterGui.AmaxGUI.HideBT.Button_Hide
local function C_1d()
local script = G2L["1d"];
	script.Parent.MouseButton1Click:Connect(function (i)
		local Menu = script.Parent.Parent.Frame
		if Menu.Visible ~= true then
			Menu.Visible = true
		else
			Menu.Visible = false
		end
	end)
end;
task.spawn(C_1d);

return G2L["1"], require;
